CREATE TABLE EmployeeHours (
    EmployeeID INT IDENTITY(1,1) PRIMARY KEY,
    LastName NVARCHAR(50),
    FirstName NVARCHAR(50),
    FullName AS (FirstName + ' ' + LastName) PERSISTED,
    JobTitle NVARCHAR(100),
    Week1Hours INT,
    Week2Hours INT,
    Week3Hours INT,
    Week4Hours INT,
    HireDate DATE
);
